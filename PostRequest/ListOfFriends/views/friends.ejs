<h1>Friends List</h1>

<ul>
    <% friends.forEach((name)=>{ %>
        <li><%= name %></li>
    <% }); %>
</ul>

<form action="/addfriend" method="POST">
    <!-- will be sent in body  -->
    <input type="text" name="newFriend" placeholder="Name">
    <button>Made New Friend</button>
</form>